apiVersion: cert-manager.io/v1
kind: Certificate
metadata:
  name: rok-docs-cert
  namespace: rok-docs
spec:
  secretName: rok-docs-cert
  issuerRef:
    name: docs-cluster-issuer
    kind: ClusterIssuer
    group: cert-manager.io
  commonName: {{ docs_host__internal }}
  dnsNames:
  - {{ docs_host__internal }}

